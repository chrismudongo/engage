<p id="notice"><%= notice %></p>

<p>
  <strong>Email:</strong>
  <%= @survey_response.email %>
</p>

<p>
  <strong>User group:</strong>
  <%= @survey_response.user_group %>
</p>

<p>
  <strong>Surveyid:</strong>
  <%= @survey_response.surveyID %>
</p>

<p>
  <strong>News media filename:</strong>
  <%= @survey_response.newsID%>

</p>

<p>
  <% if @survey_response.responses.empty? %>

  <% else %>
      <strong>Responses:</strong></br>
      <ol>


      <% for response in @survey_response.responses %>
        <li><%=h response %></li>
      <% end %>
      </ol>
    </p>
<%end%>
<%= link_to 'Back', survey_responses_path %>
